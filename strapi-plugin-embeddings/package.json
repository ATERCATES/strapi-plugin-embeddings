{
  "name": "strapi-plugin-embeddings",
  "version": "0.1.0",
  "description": "AI embeddings and semantic search plugin for Strapi v5 with pgvector support",
  "keywords": [
    "strapi",
    "plugin",
    "embeddings",
    "vector",
    "pgvector",
    "openai",
    "semantic-search",
    "ai"
  ],
  "license": "MIT",
  "author": {
    "name": "Dribo Team"
  },
  "maintainers": [
    {
      "name": "Dribo Team"
    }
  ],
  "strapi": {
    "name": "embeddings",
    "description": "AI embeddings and semantic search with OpenAI and pgvector",
    "kind": "plugin",
    "displayName": "Embeddings"
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "openai": "^4.104.0",
    "pgvector": "^0.2.1",
    "react-intl": "^7.1.14"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.27.0",
    "@strapi/typescript-utils": "^5.27.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.27.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
